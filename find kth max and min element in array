#include <iostream>
#include<stdio.h>
using namespace std;
#include<bits/stdc++.h>
int main() {
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++)
	{
	    cin>>arr[i];
	}
	sort(arr,arr+n);
	int k;
	cin>>k;
    cout<<"Min element at "<<k<<"th position is : "<<arr[k-1]<<endl;
    sort(arr,arr+n,greater<int>());
    cout<<"Max element at "<<k<<"th position is : "<<arr[k-1];
	return 0;
	
}
